{
  "expo": {
    "name": "Docker Android",
    "slug": "docker-android",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "dockerandroid",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dockerandroid.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#F6F4F1",
        "foregroundImage": "./assets/images/icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": true,
      "package": "com.dockerandroid.app"
    },
    "web": {
      "output": "single",
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#F6F4F1",
          "dark": {
            "backgroundColor": "#1A1918",
            "image": "./assets/images/icon.png"
          }
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "reactCompiler": true
    },
    "extra": {
      "eas": {
        "projectId": "1f1be5c2-d5a4-4d6c-a87d-3223f12a334d"
      }
    }
  }
}
